{
  "name": "codetex",
  "displayName": "CodeTeX",
  "description": "A fast light-weight in-line source code to LaTeX renderer for mathematical operations!",
  "version": "0.0.1",
  "publisher": "Noway-Code",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
      "onLanguage:python",
      "onLanguage:plaintext"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codetex.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "codetex.countdown",
        "title": "Countdown"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0"
  },
  "dependencies": {
    "katex": "^0.16.11",
    "which": "^5.0.0"
  }
}
